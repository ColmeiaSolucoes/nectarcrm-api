/** 
 * Schema do Objeto Formulario
 */
{
	"type": "object",
	"properties": {
		"autor": {
			"id": "autor",
			"type": "object",
			"properties": {
				"id": {
					"type": "integer"
				},
				"login": {
					"type": "string"
				},
				"nome": {
					"type": "string"
				}
			}
		},
		"autorAtualizacao": {
			"type": "object",
			"properties": {
				"id": {
					"id": "id",
					"type": "integer"
				},
				"login": {
					"type": "string"
				},
				"nome": {
					"type": "string"
				}
			}
		},
		"respostas": {
			"type": "object",
			"properties": {
				"type": "string, string",
				"description": "label, valor"
			}
		},
		"modeloFormulario": {
			"type": "object(modeloFormulario)",
			"description":"Modelo template do formulario"
		},
		"dataAtualizacao": {
			"type": "datetime",
			"description": "ISO 8601 Date format : 2016-03-30T12:00:00-03:00"
		},
		"dataCriacao": {
			"type": "datetime",
			"description": "ISO 8601 Date format : 2016-03-30T12:00:00-03:00"
		},
		"dataPreenchimento": {
			"type": "datetime",
			"description": "ISO 8601 Date format : 2016-03-30T12:00:00-03:00"
		}
	},
	"required": [
		"modeloFormulario",
		"respostas"
	]
}