		},
		"contatos": {
			"type": "array(contato)",
			"description":"Array de objetos do tipo contato (com mesmas propriedades desse schema Contato)"
		},
		"dataAtualizacao": {
			"type": "datetime",
			"description": "ISO 8601 Date format : 2016-03-30T12:00:00-03:00"
		},
		"dataCriacao": {
			"type": "datetime",
			"description": "ISO 8601 Date format : 2016-03-30T12:00:00-03:00"
		},
		"enderecos": {
			"type": "array",
			"items": {
				"type": "object"
			}
		},
		"emails": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"facebook": {
			"type": "string"
		},
		"id": {
			"type": "integer"
		},
		"indicadoPor": {
			"type": "string"
		},
