	"properties": {
		"ativo": {
			"id": "ativo",
			"type": "boolean"
		},
		"autor": {
			"id": "autor",
			"type": "object",
			"properties": {
				"id": {
					"type": "integer"
				},
				"login": {
					"type": "string"
				},
				"nome": {
					"type": "string"
				}
			}
		},
		"autorAtualizacao": {
